    "# 문제 : https://school.programmers.co.kr/learn/courses/30/lessons/42748\n",
    "\n",
    "def solution(array, commands):\n",
    "    answer = []\n",
    "    \n",
    "    for i in commands:\n",
    "        arr = array[i[0]-1: i[1]]\n",
    "        arr.sort()\n",
    "        answer.append(arr[i[2]-1])\n",
    "    return answer"
